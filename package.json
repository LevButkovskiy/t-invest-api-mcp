{
  "name": "t-invest-api",
  "version": "0.1.0",
  "description": "T-Invest API MCP Server",
  "private": true,
  "bin": {
    "t-invest-api": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "dev": "nodemon --exec ts-node src/index.ts",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "express": "^5.1.0",
    "tinkoff-invest-api": "^7.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.24",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
